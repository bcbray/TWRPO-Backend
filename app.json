{
  "name": "TWRPO-Backend",
  "website": "https://twrponly.tv/",
  "scripts": {},
  "env": {
    "DD_AGENT_MAJOR_VERSION": "7",
    "DD_API_KEY": {
      "required": true
    },
    "DD_DYNO_HOST": "true",
    "DD_SITE": {
      "required": true
    },
    "SUGGESTION_DISCORD_WEBHOOK_ID": {
      "required": true
    },
    "SUGGESTION_DISCORD_WEBHOOK_TOKEN": {
      "required": true
    },
    "TWITCH_CLIENT_ID": {
      "required": true
    },
    "TWITCH_CLIENT_SECRET": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/DataDog/heroku-buildpack-datadog.git"
    },
    {
      "url": "heroku/nodejs"
    }
  ],
  "stack": "heroku-20"
}
